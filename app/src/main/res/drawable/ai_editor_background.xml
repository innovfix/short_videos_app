<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Rounded corners to match the vector's corner radius -->
    <corners android:radius="15dp" />

    <!-- Gradient fill to match the vector's linear gradient -->
    <gradient
        android:angle="90"
        android:endColor="#1A1D1F24"
        android:startColor="#26EDCA2E"
        android:type="linear" />

    <!-- Stroke to match the vector's stroke -->
    <stroke
        android:width="1dp"
        android:color="#14FFFFFF" />

    <!-- Optional: Size to match the vector's dimensions -->
    <!--    <size-->
    <!--        android:width="110dp"-->
    <!--        android:height="100dp" />-->

</shape>